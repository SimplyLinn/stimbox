{
  "extends": "./tsconfig.base.json",
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "**/*.js",
    "**/*.jsx",
    "**/*.json"
  ],
  "compilerOptions": {
    "paths": {
      "stimbox/*": [
        "./src/*"
      ],
      "stimbox": [
        "./src"
      ]
    }
  },
  "exclude": [
    "node_modules"
  ]
}
