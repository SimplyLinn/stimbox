{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "paths": {
      "stimbox/*": [
        "./site/src/*"
      ],
      "stimbox": [
        "./site/src"
      ]
    },
    "baseUrl": "../",
    "typeRoots": [
      "../node_modules/@types",
      "../types"
    ],
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": [
    "../node_modules"
  ]
}
