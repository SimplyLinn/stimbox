{
  "name": "stimbox",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "nodemon -w tailwind.config.js -e .js -x \"next dev\"",
    "dev:prod": "yarn run build && yarn run export && light-server --config .lightrc",
    "build": "cross-env NODE_ENV=production next build",
    "export": "cross-env NODE_ENV=production shx rm -rf dist && cross-env NODE_ENV=production next export -o dist && cross-env NODE_ENV=production next-sitemap && shx mv dist/404/index.html dist/404.html && shx rm -rf dist/404 && node copy-box-static-files",
    "lint": "cross-env NODE_ENV=production run-p -cls --aggregate-output lint:*",
    "lint:eslint-check": "cross-env NODE_ENV=production eslint-config-prettier 'src/**/*.js'",
    "lint:prettier": "cross-env NODE_ENV=production prettier -c 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:eslint": "cross-env NODE_ENV=production eslint src --report-unused-disable-directives --color",
    "lint:tsc": "cross-env NODE_ENV=production tsc --noEmit --pretty",
    "gh-pages-deploy": "node scripts/gh-pages-deploy.js"
  },
  "nodemonConfig": {
    "ext": "js,jsx,ts,tsx,json,css,scss,svg",
    "watch": [
      "src",
      "next.config.js",
      ".env"
    ]
  },
  "dependencies": {
    "@simplylinn/tonematrix": "1.0.0-dev1",
    "classnames": "^2.3.0",
    "csstype": "^3.0.7",
    "elasticlunr": "^0.9.5",
    "next": "^10.1.2",
    "next-plausible": "^1.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "serve-static": "^1.14.1",
    "simplebar": "^5.3.0",
    "simplebar-react": "^2.3.0",
    "tailwindcss": "^2.0.4",
    "tone": "^14.7.77"
  },
  "devDependencies": {
    "@tailwindcss/postcss7-compat": "^2.0.2",
    "@types/classnames": "^2.2.11",
    "@types/elasticlunr": "^0.9.1",
    "@types/node": "^14.14.21",
    "@types/react": "^17.0.0",
    "@types/serve-static": "^1.13.9",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.13.0",
    "autoprefixer": "^10.2.5",
    "babel-plugin-transform-define": "^2.0.0",
    "chalk": "^4.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.18.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^7.1.0",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "execa": "^5.0.0",
    "gh-pages": "^3.1.0",
    "light-server": "^2.9.1",
    "next-sitemap": "^1.6.18",
    "node-env": "^0.1.6",
    "nodegit": "^0.27.0",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.9",
    "postcss-custom-media": "^8.0.0",
    "postcss-import": "^14.0.1",
    "postcss-mixins": "^7.0.3",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.2.1",
    "shx": "^0.3.3",
    "stylelint": "^13.8.0",
    "stylelint-config-standard": "^20.0.0",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typescript": "^4.1.3",
    "webpack": "^5.30.0"
  }
}
